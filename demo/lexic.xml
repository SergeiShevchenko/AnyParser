<?xml version="1.0" encoding="utf-8"?>
<LexicalGrammar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <CommentsList>
    <Comment Begin="{" End="}" />
  </CommentsList>
  <ReservedWords>
    <string>NE</string>
    <string>EQ</string>
    <string>LT</string>
    <string>LE</string>
    <string>GT</string>
    <string>GE</string>
    <string>plus</string>
    <string>min</string>
    <string>or</string>
    <string>mult</string>
    <string>div</string>
    <string>and</string>
    <string>true</string>
    <string>false</string>
    <string>program</string>
    <string>var</string>
    <string>begin</string>
    <string>end</string>
    <string>int</string>
    <string>float</string>
    <string>bool</string>
    <string>if</string>
    <string>then</string>
    <string>else</string>
    <string>as</string>
    <string>while</string>
    <string>do</string>
    <string>for</string>
    <string>to</string>
  </ReservedWords>
  <Separators>
    <string>~</string>
    <string>;</string>
    <string>:</string>
    <string>.</string>
    <string>,</string>
    <string>(</string>
    <string>)</string>
    <string>{</string>
    <string>}</string>
  </Separators>
  <PostProcessList>
    <PostProcessInstruction IfFound="; end" ReplaceBy="end" />
  </PostProcessList>
</LexicalGrammar>